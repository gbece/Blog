<%- include("partials/header"); -%>

<h1><b>Compose</b></h1>

<form  action="/compose" method="post">
  <div class="form-group">
    <h4><b>Title</b></h4>
    <input class=" form-control" type="text" name="postTitle" value="" placeholder="">
  </div>
  <div class="form-group">
    <h4><b>Post</b></h4>
    <textarea class="form-control"  type="text" name="postBody" rows="5" cols="30"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer"); -%>
